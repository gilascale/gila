foo fn(x: bool) -> Result do
    if x == true do
        return Result(Data=Data(data="[some data]"))
    end else do 
        return Result(Error=Error(msg="hi i have failed :()"))
    end
end

bar fn(x: bool) -> Result do
    res = !foo(x)
    print("whoo res is valid data! "+res.data)
    print("doing some processing...")
    return Result(Data=res)
end

data = bar(false)

print(data)
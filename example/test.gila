



// https://stackoverflow.com/questions/16258553/how-can-i-define-algebraic-data-types-in-python

Error type
    msg: string
end

Data type
    data: any
end

Result = Error | Data

foo fn(x:string) -> Result do
    return Result(data=x)
end

x = !foo("hi")


e = Error(msg="there has been an error")

print(e)

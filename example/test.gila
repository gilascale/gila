



// https://stackoverflow.com/questions/16258553/how-can-i-define-algebraic-data-types-in-python

Error type
    msg: string
end

Data type
    data: any
end

Result = Error | Data

foo fn() -> Result do
    return Result(Error=Error(msg="hi i have failed :()"))
end

bar fn() -> Result do
    res = !foo()
    return Result(Data=res)
end


data = bar()

print(data)


